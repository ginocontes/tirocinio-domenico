   "outputs": [],
   "source": [
    "def list_gcs_files(bucket_name, prefix=''):\n",
    "    storage_client = storage.Client()\n",
    "    bucket = storage_client.get_bucket(bucket_name)\n",
    "    blobs = bucket.list_blobs(prefix=prefix)\n",
    "    files = [blob.name for blob in blobs]\n",
    "    return files"
   ]
  }
